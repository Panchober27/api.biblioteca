{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "no-console": false,
    "variable-name": [false, "allow-leading-underscore", "allow-snake-case"],
    "radix": false,
    "triple-equals": false,
    "no-empty": false,
    "prefer-for-of": false,
    "forin": false,
    "no-shadowed-variable": false,
    "no-var-requires": false,
    "only-arrow-functions" : false,
    "no-unused-expression" : false,
    "prefer-const" : false
  },
  "rulesDirectory": []
}